<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.userappnavigationdrawer.ui.home.homeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home">
    </fragment>

    <fragment
        android:id="@+id/navigation_notice"
        android:name="com.example.userappnavigationdrawer.ui.notice.noticeFragment"
        android:label="Notice"
        tools:layout="@layout/fragment_notice">
    </fragment>

    <fragment
        android:id="@+id/navigation_about"
        android:name="com.example.userappnavigationdrawer.ui.about.aboutFragment"
        android:label="About"
        tools:layout="@layout/fragment_about">
    </fragment>

    <fragment
        android:id="@+id/navigation_gallery"
        android:name="com.example.userappnavigationdrawer.ui.gallery.galleryFragment"
        android:label="Gallery"
        tools:layout="@layout/fragment_gallery">
    </fragment>

</navigation>